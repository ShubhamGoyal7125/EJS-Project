<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">

    <div class="mb-3 form-group">
<label for="composeTitle" class="form-label">Title</label>
<input type="text" name="composeTitle" class="form-control" id="composeTitle">
</div>

<div class="mb-3 form-group">
<label for="composeText" class="form-label">Post</label>
<textarea name="composeText" id="composeText" class="form-control" cols="30" rows="5"></textarea>
</div>

<button type="submit" class="btn btn-primary">Publish</button>

</form>

<%- include("partials/footer") -%>